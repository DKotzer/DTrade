<div class="buyArea">
  <div class="subheader">
    <div class="row">
      <div>Account: <%=account.number%></div>
      <div>
        Total Value:
        <%=account.totalValue.toFixed(2).replace(/\d(?=(\d{3})+\.)/g, '$&,')%>
      </div>
    </div>
    <br />
    <div class="row">
      <div>
        Buying Power: <%=account.cash.toFixed(2).replace(/\d(?=(\d{3})+\.)/g,
        '$&,')%>
      </div>
      <div>
        Market Value:
        <%=account.marketValue.toFixed(2).replace(/\d(?=(\d{3})+\.)/g, '$&,')%>
      </div>
    </div>
  </div>

  <div>graph</div>
  <img src="https://i.imgur.com/EFqRbev.png" alt="stonks" />
  <P> </P>
  <p></p>
  <h1>Sell</h1>
  <form class="buyForm" action="/trade/sell" method="post">
    <label for="symbol">Symbol</label>
    <input type="text" name="symbol" placeholder="symbol" />
    <label for="">Price</label>
    <input type="number" name="price" placeholder="price" />
    <label for="shares">shares</label>
    <input type="number" name="shares" placeholder="shares" />
    <label for="total">Total</label>
    <input type="number" name="total" placeholder="total" />
    <br />
    <div><button type="submit">Sell</button></div>
  </form>
</div>
